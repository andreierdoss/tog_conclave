<h1><%= link_to I18n.t("tog_conclave.plugin_name"), admin_bookmarks_addresses_path%> /  <%= I18n.t("tog_conclave.admin.create_event") %></h1>



<%= error_messages_for :event %>
<% form_for :event, :url => admin_conclave_events_path,  :html => {:id => "settings"} do |f| -%>

  <%= render :partial => 'form', :locals => { :f => f } %>

  <p>
    <br />
    <%= submit_tag I18n.t("tog_conclave.admin.create_event"), {:accesskey => "s", :class=>"button"} %>
  </p>
  
<% end -%>
